<h1>Restaurants</h1>
<ul>
<% @restaurants.each do |restaurant| %>
  <li><strong><%= link_to(restaurant.name, restaurant_path(restaurant)) %></strong> | <%= restaurant.address %> | <i> <%= restaurant.category %></i></li>
<% end %>
<br>
<%= link_to "add a restaurant", new_restaurant_path, class: "btn btn-primary" %>
